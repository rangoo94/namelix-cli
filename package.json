{
  "name": "namelix-cli",
  "version": "1.0.0",
  "description": "Generate business company names based on keywords via Namelix.",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "namelix": "./src/cli.js",
    "namelix-cli": "./src/cli.js"
  },
  "main": "./index",
  "types": "./index.d.ts",
  "scripts": {
    "clean": "rimraf src/**/*.{js,d.ts} index.{js,d.ts}",
    "build": "npm run clean && tsc -p ./tsconfig.json",
    "ts-check": "tsc -p ./tsconfig.json --noEmit",
    "test": "npm run ts-check",
    "prepublishOnly": "npm run build"
  },
  "homepage": "https://github.com/rangoo94/namelix-cli#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rangoo94/namelix-cli.git"
  },
  "bugs": {
    "url": "https://github.com/rangoo94/namelix-cli/issues"
  },
  "keywords": [
    "namelix",
    "name",
    "generator",
    "random",
    "business",
    "company"
  ],
  "author": {
    "name": "Dawid Rusnak",
    "email": "dawid@drcode.pl",
    "url": "https://www.drcode.pl"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "src/*.js",
    "src/*.d.ts",
    "index.js",
    "index.d.ts"
  ],
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.2.0",
    "got": "^11.8.3",
    "progress": "^2.0.3"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/node": "^17.0.29",
    "@types/progress": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4"
  }
}
